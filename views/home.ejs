<%- include("partials/header") -%>

<h1>Home</h1>
<p><%- StartingContent %></p>

<% posts.forEach((post) => { %>
<h2 style="color: rgb(57, 57, 58)"><%= post.title %></h2>
<p>
  <%- post.content.substring(0,100)+"&hellip;" %>
  <a href="/posts/<%= post._id %>">Read More</a>
</p>
<% }) %> <%- include("partials/footer") -%>
